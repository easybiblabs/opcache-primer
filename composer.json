{
    "name": "easybib/opcache-primer",
    "description": "Tooling to prime Zend OPcache during deploys",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Till Klampaeckel",
            "email": "till@php.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "EasyBib\\OPcache": "src"
        }
    },
    "bin": [
        "bin/opcache-primer.php"
    ],
    "require": {
        "PHP": ">=5.5.0",
        "ext-Zend-OPcache": "*",
        "crunch/fastcgi": "v1.0.1",
        "psr/log": "1.*",
        "symfony/console": "^2.7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mikey179/vfsStream": "*"
    },
    "suggest": {
        "monolog/monolog": "1.3.0+ for logging!"
    }
}
